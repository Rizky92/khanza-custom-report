{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-xlswriter": "*",
        "diglactic/laravel-breadcrumbs": "^8.1.1",
        "fideloper/proxy": "^4.4.2",
        "fruitcake/laravel-cors": "^2.2",
        "guzzlehttp/guzzle": "^7.7.0",
        "infyomlabs/routes-explorer": "^1.0",
        "lab404/laravel-impersonate": "^1.7.4",
        "laravel/framework": "^8.83.27",
        "laravel/tinker": "^2.8.1",
        "livewire/livewire": "<=2.10.7",
        "nullpunkt/lz-string-php": "^1.3",
        "rap2hpoutre/laravel-log-viewer": "^2.3",
        "rizky92/carbon-helper": "^0.0.2",
        "rizky92/laravel-xlswriter": "*",
        "spatie/laravel-permission": "^5.10.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.13.0",
        "facade/ignition": "^2.17.7",
        "fakerphp/faker": "^1.22.0",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^4.3",
        "phpunit/phpunit": "^9.6.8",
        "psalm/plugin-laravel": "^1.6.2",
        "staudenmeir/laravel-migration-views": "^1.5.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan livewire:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models --nowrite",
            "@php artisan ide-helper:meta"
        ],
        "psalm-init": [
            "./vendor/bin/psalm --init",
            "./vendor/bin/psalm-plugin enable psalm/plugin-laravel"
        ],
        "analyze": [
            "./vendor/bin/psalm > analysis.txt"
        ],
        "dry-run": [
            "./vendor/bin/psalm --alter --issues=all --dry-run > dry-run.txt"
        ]
    }
}
